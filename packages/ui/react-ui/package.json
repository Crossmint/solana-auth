{
  "name": "@crossmint/solana-auth-react-ui",
  "version": "0.1.3-alpha.2",
  "author": "Paella Labs Inc.",
  "license": "Apache-2.0",
  "repository": "https://github.com/Paella-Labs/solana-firebase-auth",
  "type": "module",
  "sideEffects": false,
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.mjs",
  "types": "lib/esm/index.d.ts",
  "files": [
    "lib",
    "src",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./lib/esm/index.mjs",
      "require": "./lib/cjs/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "shx rm -rf lib/*",
    "compile": "tsc-esm",
    "build": "yarn clean && tsc -p tsconfig.json; yarn run tsc-esm -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "postbuild": "echo '{\"type\":\"commonjs\"}' | npx json > lib/cjs/package.json && echo '{\"type\":\"module\"}' | npx json > lib/esm/package.json"
  },
  "dependencies": {
    "@crossmint/solana-auth-base": "^0.1.3-alpha.1",
    "@solana/wallet-adapter-base": "^0.9.2",
    "@solana/wallet-adapter-react": "^0.15.2",
    "@solana/wallet-adapter-react-ui": "^0.9.4",
    "@solana/wallet-adapter-wallets": "^0.14.2",
    "@solana/web3.js": "^1.35.1",
    "firebase": "^9.6.7",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-firebase-hooks": "^5.0.3",
    "tsc-esm": "^1.0.4",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0"
  }
}
